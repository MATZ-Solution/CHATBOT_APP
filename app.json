{
  "expo": {
    "name": "chatbot",
    "slug": "chatbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/chatbot.png",
    "scheme": "chatbot",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/chatbot.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.infoseniorcare.chatbot"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "CUSTOM: Allow Voice To Text to access the microphone",
          "speechRecognitionPermission": "CUSTOM: Allow Voice To Text to securely recognize user speech"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1fd429f8-dae8-4e6b-8de4-a301ab229420"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1fd429f8-dae8-4e6b-8de4-a301ab229420"
    }
  }
}
